{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "moduleResolution": "node"
    },
    "include": [
        "./src/**/*",
        "./server.ts"
    ],
    "exclude": [
        "./typings/index.d.ts",
        "./node_modules/**/*",
        "./typings/**/*"
    ]
}
